%import ../src/lib/ipckernel.alo
%import ../src/lib/io.alo

func main() {
    ipckernel k;
    int a = 42;
    int b = 24;
    __compile__(&k,""""
    import time
    time.sleep(1)
    print(a)
    print(b)
    b += a
"""");
    print(a);
    print(b);
    print("Call Python");
    __call__(&k);
    print("Call finished");
    print(a);
    print(b);
}